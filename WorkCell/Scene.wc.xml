	</Frame>
	<Frame name="GraspTargetCylinderCenter" refframe="Cylinder" type="Fixed">
			<RPY>0 0 -90</RPY> <Pos>0 -0.1 -0.10</Pos> <!-- From side is RBY 0 -90 0 pos 0.05 0 0, ///// From top:  0 90 90  pos 0 0.1 0 -->
	</Frame>


	<Frame name="Place" refframe="Table" type="Fixed" daf="true">
