	</Frame>



	<Frame name="GraspTargetBottleCenter" refframe="Bottle" type="Fixed">
			<RPY>0 0 280</RPY> <Pos>0 -0.075 -0.1</Pos> 
	</Frame>
	






	<Frame name="Square" refframe="Table" type="Movable" daf="true">
