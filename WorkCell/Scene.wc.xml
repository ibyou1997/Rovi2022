<WorkCell name="Scene">

	<Frame name="Table" refframe="WORLD" type="Fixed">
		<RPY>0 0 0</RPY>
		<Pos>0 0 -0.1</Pos>
		<Drawable name="Table" refframe="Table">
			<Polytope file="table/Sigmund_table" />
		</Drawable>
	</Frame>

	<Frame name="URReference" refframe="Table" type="Movable">
		<RPY>0 0 0</RPY>
		<Pos>0.0 0.0 0.11</Pos><!--z max is +- 0.3 the first one, the second one is 0.2- MOvTO forhold til world cordinate, SETTransform forhod til tabel -->

	</Frame>
